		{
			"name": "Create User",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.environment.set(\"email\", jsonData.email);\r",
							"pm.environment.set(\"userId\", jsonData.id);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"response matches the Json schema\",function(){\r",
							"\tconst jsonData = pm.response.json();\r",
							"\tconst schema = {\r",
							"        \"type\": \"object\",\r",
							"        \"properties\": {\r",
							"            \"id\": {\"type\": \"string\"},\r",
							"            \"title\":{\"type\": \"string\", \"enum\": [\"mr\", \"mrs\", \"ms\", \"miss\", \"dr\", \"\"]},\r",
							"            \"firstName\": {\"type\": \"string\", \"minLenght\": 2, \"maxLength\": 50},\r",
							"            \"lastName\": {\"type\": \"string\", \"minLength\": 2, \"maxLength\": 50},\r",
							"            \"gender\": {\"type\": \"string\", \"enum\": [\"male\", \"female\", \"other\", \"\"]},\r",
							"            \"email\": {\"type\": \"string\", \"format\": \"email\"},\r",
							"            \"dateOfBirth\": {\"type\": \"string\", \"format\": \"date\", \"minimum\": \"1900-01-01T00:00:00Z\"},\r",
							"            \"registerDate\": {\"type\": \"string\"},\r",
							"            \"phone\": {\"type\": \"string\"},\r",
							"            \"picture\": {\"type\": \"string\", \"format\": \"url\"},\r",
							"            \"location\": {\"type\": \"object\"}\r",
							"        },\r",
							"        \"required\": [\"firstName\", \"lastName\", \"email\"]\r",
							"\r",
							"\t};\r",
							"\tpm.expect(tv4.validate(jsonData, schema)).to.be.true;\r",
							"});\r",
							"\r",
							"// Получение данных из ответа\r",
							"const responseJson = pm.response.json();\r",
							"\r",
							"// Регулярное выражение для проверки наличия цифр\r",
							"const regex = /\\d/;\r",
							"\r",
							"// Проверка, что поле `firstName` не содержит цифр\r",
							"pm.test(\"Field 'firstName' does not contain numbers\", function () {\r",
							"    pm.expect(regex.test(responseJson.firstName)).to.eql(false);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
